# .env file for Jarvis Voice Assistant

# Backend server port
PORT=5000

# Frontend development server port (set by create-react-app)
# PORT=3000

# Google OAuth credentials
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
GOOGLE_REDIRECT_URI=http://localhost:3000/auth/callback

# AI Provider configuration (openai or claude)
AI_PROVIDER=openai
AI_API_KEY=your-openai-api-key
# AI_API_KEY=your-claude-api-key

# Offline mode for development/testing without API calls
# REACT_APP_OFFLINE_MODE=true

# Node environment
NODE_ENV=development
# NODE_ENV=production